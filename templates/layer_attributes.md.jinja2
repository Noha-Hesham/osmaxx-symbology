{% if dicts_len > 1 %}
{{ header }}
{% for attribute_name, attribute in attributes.items() -%}|
    {{- attribute_name }}|
    {{- attribute['type'] }}|
    {{- attribute['description']|default(' ') }}|
    {%- if attribute['osm_tags'] is defined -%}
        `{{ attribute['osm_tags'][0].keys()[0] }}={{ attribute['osm_tags'][0].values()[0] }}`
    {%- elif 'values' in attribute -%}
        (see table below)
    {%- else -%}
        {{ ' ' }}
    {%- endif %}|
{% endfor %}
{% endif -%}
